{
  "name": "Password Manager",
  "description": "A Password Manager for KaiOS",
  "start_url": "/index.html",
  "theme": "#ff3e00",
  "theme_color": "#ff3e00",
  "background_color": "#ff3e00",
  "display": "fullscreen",
  "orientation": "default",
  "categories": [
    "utilities"
  ],
  "icons": [
    {
      "src": "/icons/icon56x56.png",
      "type": "image/png",
      "sizes": "56x56"
    },
    {
      "src": "/icons/icon112x112.png",
      "type": "image/png",
      "sizes": "112x112"
    }
  ],
  "b2g_features": {
    "version": "3.0.0",
    "type": "web",
    "core": true,
    "default_locale": "en-US",
    "developer": {
      "name": "arma7x",
      "url": "https://github.com/arma7x/kaios-svelte-starter"
    },
    "chrome": {
      "statusbar": "overlap"
    },
    "origin": "password-manager",
    "permissions": {
      "video-capture": {
        "description": "Required for qr-code reader"
      }
    },
    "dependencies": {},
    "locales": {
      "en-US": {
        "name": "Password Manager",
        "subtitle": "A Password Manager for KaiOS",
        "description": "A Password Manager for KaiOS"
      }
    }
  }
}
